<h1>Add a New Plant</h1>
<%= form_with url: "/gardens/#{@garden.id}/plants", method: :post, data: { turbo: false } do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :height %>
  <%= form.number_field :height %>

  <%= form.label :flowering %>
  <%= form.check_box :flowering %>
  <%= form.submit 'Create Plant' %>
<% end %>